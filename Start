from selenium import webdriver
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.by import By
from selenium.webdriver.support.ui import WebDriverWait
from selenium.webdriver.support import expected_conditions as EC
import time

PATH = "C:\Program Files (x86)/chromedriver"
driver = webdriver.Chrome(PATH)

driver.get("http://www.b3.com.br/pt_br/")
#print (driver.title)#check the title


search = driver.find_element_by_id("query") #acha a caixa de texto
search.send_keys("ITSA4") #escreve na caixa de texto
search.send_keys(Keys.RETURN) #aperta enter

